<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
	xml:base ="http://reecedunn.co.uk/tts/synthesizer/mbrola"
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:tts="http://reecedunn.co.uk/schema/2014/tts#"
	xmlns:dc ="http://purl.org/dc/elements/1.1/"
>

<tts:Voice rdf:ID="en1">
	<tts:name>en1</tts:name>
	<tts:phonemeset>mbrola/en1</tts:phonemeset>
	<tts:gender rdf:resource="http://reecedunn.co.uk/schema/2014/tts#male"/>
	<!--
		The voice samples produce a maximum default volume of 0.25
		(excluding data spikes). Therefore, a scale factor of 2 is
		needed to adjust that to 0.5.
	-->
	<tts:volumeScale>2.0</tts:volumeScale>
</tts:Voice>

</rdf:RDF>
