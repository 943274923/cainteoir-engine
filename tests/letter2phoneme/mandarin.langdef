.locale		en
.phonemeset	cxs

rewrite

	謝	"xiè "
	多	"duō "

end

rules

		x	/s\/
		i	/j_^/
		è	/E_T_B/

		d	/t/
		u	/u_^/
	u)	ō	/O_T/

end
