.locale		en
.phonemeset	cxs

rules

	# a #################

	ab	/@b/
	a	/&/

	# b #################

	b	/b/

	# c #################

	c (a	/s/
	c	/k/

	# d #################

	u) d	/f/
	   d	/d/

	# f #################

	_) f	/T/
	   f (_	/v/
	   f	/f/

	# u #################

	u	/@/

end
