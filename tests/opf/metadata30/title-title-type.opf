<package xmlns="http://www.idpf.org/2007/opf" version="3.0">
	<metadata xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:opf="http://www.idpf.org/2007/opf">
		<dc:title prefer="main">Main Title</dc:title>

		<meta property="dcterms:title" id="main">Main Title</meta>
		<meta refines="#main" property="title-type">primary</meta>

		<meta property="dcterms:title" id="coll">The Collection</meta>
		<meta refines="#coll" property="title-type">collection</meta>

		<meta property="dcterms:title" id="series">The Series</meta>
		<meta refines="#series" property="title-type">series</meta>

		<meta property="dcterms:title" id="ed">3rd Edition</meta>
		<meta refines="#ed" property="title-type">edition</meta>
	</metadata>
</package>
